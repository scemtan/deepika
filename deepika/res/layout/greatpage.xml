<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/todo"
        android:src="@drawable/deepika" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:src="@drawable/iletisim" />

    <TextView
        android:id="@+id/texcredit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textcustomer"
        android:layout_below="@+id/textcustomer"
        android:layout_marginTop="10dp"
        android:text="Credit : €"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/texcredit"
        android:layout_alignRight="@+id/aldi_talk"
        android:text="#null"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/texttransaction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/texcredit"
        android:layout_below="@+id/texcredit"
        android:layout_marginTop="10dp"
        android:text="Number of Transactions:"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/textdailyturnover"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/texttransaction"
        android:layout_below="@+id/texttransaction"
        android:layout_marginTop="10dp"
        android:text="Daily Turnover: €"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textdailyturnover"
        android:layout_alignRight="@+id/textView6"
        android:text="#null"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/textbankbalance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textdailyturnover"
        android:layout_below="@+id/textdailyturnover"
        android:layout_marginTop="10dp"
        android:text="Bank Balance: €"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/button2"
        android:layout_below="@+id/button2"
        android:layout_marginTop="15dp"
        android:background="#0381CD"
        android:text="ORDER HISTORY"
        android:textColor="#ffffff"
        android:textSize="15dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/button3"
        android:layout_below="@+id/button3"
        android:layout_marginTop="15dp"
        android:background="#0381CD"
        android:text="PRINT TEST CARD"
        android:textColor="#ffffff"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button1"
        android:layout_alignLeft="@+id/textView7"
        android:text="#null"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/aldi_talk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textcustomer"
        android:layout_alignBottom="@+id/textcustomer"
        android:layout_alignRight="@+id/button1"
        android:text="#null"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/textcustomer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_below="@+id/imageView1"
        android:layout_marginTop="15dp"
        android:text="Customer No:"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button4"
        android:layout_alignTop="@+id/imageView4"
        android:src="@drawable/bilgi" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/button4"
        android:layout_marginBottom="27dp"
        android:src="@drawable/cikis" />

    <Button
        android:id="@+id/button5"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/button2"
        android:layout_below="@+id/button1"
        android:layout_marginTop="15dp"
        android:background="#73d700"
        android:text="FLIXBUS"
        android:textColor="#ffffff"
        android:textSize="15dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/button1"
        android:layout_below="@+id/button5"
        android:layout_marginTop="15dp"
        android:background="#0381CD"
        android:text="CHANGE PASSWORD"
        android:textColor="#ffffff"
        android:textSize="15dp" />

    <Button
        android:id="@+id/button1"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_below="@+id/textbankbalance"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="#0381CD"
        android:text="ONLINE CARD"
        android:textColor="#ffffff"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textdailyturnover"
        android:layout_alignLeft="@+id/textView2"
        android:text="#null"
        android:textColor="#0381CD"
        android:textColorHint="#0381CD"
        android:textSize="10dp" />

</RelativeLayout>